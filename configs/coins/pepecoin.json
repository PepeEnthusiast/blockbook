{
    "coin": {
        "name": "Pepecoin",
        "shortcut": "PEPE",
        "label": "Pepecoin",
        "alias": "pepecoin"
    },
    "ports": {
        "backend_rpc": 8038,
        "backend_message_queue": 38338,
        "blockbook_internal": 9038,
        "blockbook_public": 9138
    },
    "ipc": {
        "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
        "rpc_user": "rpc",
        "rpc_pass": "rpcp",
        "rpc_timeout": 25,
        "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
    },
    "backend": {
        "package_name": "backend-pepecoin",
        "package_revision": "satoshilabs-1",
        "system_user": "pepecoin",
        "version": "1.0.0",
        "binary_url": "https://github.com/pepecoinppc/pepecoin/releases/download/v1.0.0/pepecoin-1.0.0-x86_64-linux-gnu.tar.gz",
        "verification_type": "sha256",
        "verification_source": "90900d9774b60280c5e9e0b75d3e1d566fd2040906a2bf2bc0a2b2c0a9f38e3a",
        "extract_command": "tar -C backend --strip 1 -xf",
        "exclude_files": ["bin/pepecoin-qt"],
        "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/pepecoind -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
        "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
        "postinst_script_template": "",
        "service_type": "forking",
        "service_additional_params_template": "",
        "protect_memory": false,
        "mainnet": true,
        "server_config_file": "bitcoin_like.conf",
        "client_config_file": "bitcoin_like_client.conf",
        "additional_params": {
            "discover": 0,
            "rpcthreads": 16,
            "upnp": 0,
            "whitelist": "127.0.0.1"
        },
        "platforms": {
            "arm64": {
                "binary_url": "https://github.com/pepecoinppc/pepecoin/releases/download/v1.0.0/pepecoin-1.0.0-aarch64-linux-gnu.tar.gz",
                "verification_source": "bf1a40876a90d3f2d7ad907e952b96d7ebf712e7a27f7255ecb84e0dce2b789d",
                "exclude_files": []
            }
        }
    },
    "blockbook": {
        "package_name": "blockbook-pepecoin",
        "system_user": "blockbook-pepecoin",
        "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
        "public_binding_template": ":{{.Ports.BlockbookPublic}}",
        "explorer_url": "",
        "additional_params": "-resyncindexperiod=30011 -resyncmempoolperiod=2011",
        "block_chain": {
            "parse": true,
            "mempool_workers": 8,
            "mempool_sub_workers": 2,
            "block_addresses_to_keep": 300,
            "xpub_magic": 49990397,
            "slip44": 3,
            "additional_params": {
                "fiat_rates": "coingecko",
                "fiat_rates_vs_currencies": "USD",
                "fiat_rates_params": "{\"coin\": \"pepecoin-network\", \"periodSeconds\": 900}"
            }
        }
    },
    "meta": {
        "package_maintainer": "IT Admin",
        "package_maintainer_email": "it@satoshilabs.com"
    }
}
